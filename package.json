{
  "name": "something-borrowed",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cloudinary": "^1.30.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "mongoose": "^6.4.4"
  },
  "scripts": {
    "start-frontend": "cd frontend && npm start",
    "start-server": "node index.js",
